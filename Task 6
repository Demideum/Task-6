namespace IJunior_Task6
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int picturesInRow = 3;
            int totalPictures = 53;
           
            int fullRow = totalPictures / picturesInRow;
            int surplusPictures = totalPictures % picturesInRow;

            Console.WriteLine("Полностью заполненных рядов можно вывести " + fullRow + ", картинок сверх меры " + surplusPictures);
            
            Console.ReadKey();
        }
    }
}
